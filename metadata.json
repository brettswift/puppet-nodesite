{
  "name": "brettswift-nodesite",
  "version": "0.0.3",
  "author": "Brett Swift",
  "summary": "Nodejs web app deployments",
  "license": "MIT",
  "source": "git://github.com/brettswift/puppet-nodesite",
  "project_page": "http://github.com/brettswift/puppet-nodesite",
